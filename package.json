{
    "name": "debts",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "node dist/server",
        "build": "npm run build:backend && npm run build:frontend",
        "build:prod": "npm run build:backend && npm run build:frontend:prod",
        "build:frontend": "cd frontend && npm i && npm run build",
        "build:frontend:prod": "cd frontend && npm i && npm run build:prod",
        "build:backend": "npm run tsoa:gen && tsc",
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "npm run tsoa:gen && concurrently \"tsc -w\" \"nodemon --ignore './frontend/' --inspect=0.0.0.0 dist/server\"",
        "dev:frontend": "cd frontend && npm start",
        "gen-routes": "tsoa routes",
        "gen-swagger": "tsoa swagger",
        "tsoa:gen": "npm run gen-routes && npm run gen-swagger",
        "postinstall": "npm run build",
        "heroku-postbuild": ""
    },
    "private": false,
    "dependencies": {
        "@nebular/moment": "^4.1.1",
        "@types/async": "^2.0.49",
        "@types/express": "^4.16.0",
        "@types/express-session": "^1.15.10",
        "@types/node": "^9.6.23",
        "async": "^2.6.4",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "core-js": "^2.5.7",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-jwt": "^8.4.1",
        "jsonwebtoken": "^8.3.0",
        "morgan": "^1.8.2",
        "mysql2": "^3.2.0",
        "ngx-papaparse": "^3.0.3",
        "nodemailer": "^4.6.7",
        "pbkdf2": "^3.0.17",
        "reflect-metadata": "^0.1.12",
        "return-deep-diff": "^0.3.0",
        "sequelize": "^5.8.12",
        "sequelize-typescript": "^1.0.0-beta.3",
        "swagger-ui-express": "^4.6.2",
        "tsoa": "^2.4.3",
        "typescript": "^3.5.2",
        "typings": "^2.1.1"
    },
    "devDependencies": {
        "concurrently": "^3.6.0",
        "nodemon": "^1.18.1",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1"
    }
}
